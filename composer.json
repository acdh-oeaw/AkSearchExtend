{
    "name": "acdh-oeaw/aksearch-ext",
    "description": "A sample extension module for VuFind/AkSearch",
    "keywords": [],
    "homepage": "https://github.com/acdh-oeaw/AkSearchExtend",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Żółtak",
            "email": "mzoltak@oeaw.ac.at"
        }
    ],
    "require": {
        "php": ">=8.1",
        "vufind-org/vufind-marc": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "aksearchExt\\": "src/aksearchExt"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "vufind/vufind": "^10",
        "ahand/mobileesp": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}
